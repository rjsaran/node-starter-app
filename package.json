{
  "name": "node-starter-app",
  "version": "1.0.0",
  "description": "Node Starter App",
  "main": "dist/main.js",
  "scripts": {
    "lint": "eslint src --quiet",
    "lintfix": "eslint src --fix",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "prestart": "npm run db:seed",
    "start": "node dist/main.js",
    "start:dev": "npm run db:seed & nodemon",
    "test": "jest --detectOpenHandles --force-exit",
    "db:seed": "ts-node src/scripts/db-seed.ts",
    "docker:build": "docker-compose build",
    "docker:run": "docker-compose up",
    "docker:build:run": "docker-compose up --build",
    "docker:stop": "docker-compose down"
  },
  "author": "Ramjeet Saran",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.12.0",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslint": "^5.20.1",
    "typescript": "^5.7.3"
  }
}
